[user]
	email = bdellaterra@users.noreply.github.com
	name = Brian Dellaterra
[core]
	autocrlf = false
	safecrlf = false
	ignorecase = false
	excludesfile = ~/.gitignore
[branch]
	autosetuprebase = always
[diff]
	renames = copies
[color]
	ui = true
[alias]
	a = add
	aa = !git add . && git commit -am
	b = branch 
	ca = commit --amend -m
	ci = commit
	cl = clone
	cm = commit -m
	cob = checkout -b
	co = checkout
	cp = cherry-pick
	d = diff --word-diff
	dc = diff --cached
	ds = diff --stat
	rao = remote add origin
	m = submodule
	mi = !git submodule init && git submodule update
	mu = submodule foreach git pull origin master
	p = push origin master
	pb = push -u origin --all
	s = status -s
	st = status
	sl = stash list
	sa = stash apply
	ss = stash save
	u = pull

	# Condensed log
	l = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --graph --decorate --date=short

	# Log showing changed files
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat

	# list aliases
	aliases = "!git config -l | grep alias | cut -c 7-"

