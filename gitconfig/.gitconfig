[user]
	email = bdellaterra@users.noreply.github.com
	name = Brian Dellaterra
[core]
	autocrlf = false
	safecrlf = false
	ignorecase = false
	excludesfile = ~/.gitignore
[branch]
	autosetuprebase = always
[diff]
	renames = copies
[color]
	ui = true
[alias]
	a = add
	aa = !git add . && git commit -am
	b = branch 
	bd = branch -d
	bm = branch -m
	ca = commit --amend
	ci = commit
	cl = clone
	cm = commit -m
	cob = checkout -b
	cot = "!f() { git checkout tags/$1; }; f"
	co = checkout
	d = diff --word-diff
	dc = diff --cached
	ds = diff --stat
	ec = config --global -e
	f = fetch
	ft = fetch --tags
	rao = remote add origin
	m = submodule
	ma = submodule add
	mi = !git submodule init && git submodule update
	mu = submodule foreach git pull origin master
	pl = pull
	pu = push origin master
	pb = push -u origin --all
	pk = cherry-pick
	s = status -s
	st = status
	sl = stash list
	sa = stash apply
	ss = stash save
	t = tag
	ta = tag -a

	# Condensed log
	l = log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --graph --decorate --date=short

	# Log showing changed files
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat

	# remove local branches that have already been merged to master (Credit to haacked)
	bclean = "!f() { git branch --merged ${1-master} | grep -v " ${1-master}$" | xargs -r git branch -d; }; f"

	# see what new commits have been created by the last command
	new = !sh -c 'git log $1@{1}..$1@{0} "$@"'

	# list aliases
	aliases = "!git config -l | grep alias | cut -c 7-"

